    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def log_softmax(scores: list) -> np.ndarray:\n",
    "    denominator = sum([np.exp(i) for i in scores])\n",
    "    return np.array([np.log(np.exp(i) / denominator) for i in scores])"
   ]
  },

   "metadata": {},
   "outputs": [],
   "source": [
    "def softmax(scores: list[float]) -> list[float]:\n",
    "    denominator = sum([math.exp(i) for i in scores])\n",
    "    return [math.exp(i) / denominator for i in scores]"
   ]
